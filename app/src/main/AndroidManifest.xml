<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="devloper.game.sqldatabasefinishlearn">

    <application
        android:allowBackup="true"
        android:icon="@drawable/sql"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Database_Account.ResetPassword" />
        <activity android:name=".BASIC_PART.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BASIC_PART.Restore" />
        <activity android:name=".Database_Account.RegisterActivity" />
        <activity android:name=".Database_Account.LoginActivity" />
        <activity android:name=".BASIC_PART.MainActivity" />
    </application>

</manifest>